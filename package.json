{
  "name": "mood-mingle",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:all": "./scripts/run-all-tests.sh",
    "ios:setup": "./scripts/setup-ios-testing.sh",
    "ios:open": "npm run build && npx cap sync ios && npx cap open ios",
    "test:pwa": "./scripts/test-pwa-iphone.sh",
    "servers:restart": "./scripts/restart-servers.sh",
    "servers:stop": "pkill -f vite && pkill -f 'node.*server/index.js'"
